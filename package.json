{
  "name": "lighter",
  "version": "0.3.0",
  "description": "A lightweight Node.js framework",
  "dependencies": {
    "beams": "^0.1.0",
    "cedar": "^0.1.3",
    "chug": "^0.2.2",
    "d6": "^0.1.8",
    "jymin": "^0.4.0",
    "ltl": "^0.1.15",
    "mc": "^1.0.6",
    "ormy": "^0.3.0",
    "shellify": "^0.0.5",
    "splode": "^0.0.2",
    "za": "^0.1.1"
  },
  "devDependencies": {
    "exam": "^0.1.6",
    "istanbul": "0.3.2",
    "coveralls": "2.11.1",
    "express": "4.9.0",
    "zeriousify": "^0.1.12"
  },
  "keywords": [
    "lighter",
    "isomorphic",
    "mvc",
    "http",
    "https",
    "router",
    "express",
    "alternative"
  ],
  "main": "lighter.js",
  "bin": {
    "lighter": "lighter.js"
  },
  "engines": {
    "node": ">=0.2.6"
  },
  "scripts": {
    "test": "./node_modules/exam/exam.js",
    "cover": "istanbul cover ./node_modules/exam/exam.js",
    "report": "open coverage/lcov-report/index.html",
    "coveralls": "istanbul cover ./node_modules/exam/exam.js --report lcovonly -- -R spec && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage"
  },
  "repository": "https://github.com/lighterio/lighter.git",
  "bugs": "https://github.com/lighterio/lighter/issues",
  "homepage": "http://lighter.io/lighter",
  "author": "Sam Eubank <sameubank@gmail.com>",
  "contributors": [
    "Sam Eubank <sameubank@gmail.com>"
  ],
  "license": "MIT"
}
